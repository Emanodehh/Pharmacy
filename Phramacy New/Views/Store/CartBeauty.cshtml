@model IEnumerable<Phramacy_New.DTO.StoreDTO>
@if (Model.Count() > 0 && Model != null)
{
    <table class="table table-dark m-2">
        <thead>
            <tr>
                <th scope="col">Beauty Name</th>
                <th scope="col">Price</th>
                <th scope="col">Store Date</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var s in Model)
            {
                <tr>
                    <td>@s.BeautyName</td>
                    <td>$@s.Price</td>
                    <td>
                        @if (!String.IsNullOrEmpty(s.ImageName))
                        {
                            <img width="100" height="100" src="~/BeautyImages/@s.ImageName" />
                        }
                        else
                        {
                            <p>No Image</p>
                        }
                    </td>
                    <td>@s.CreatedDate.ToString("yyyy-MM-dd  hh:mm:tt")</td>
                </tr>
            }



        </tbody>
    </table>
}
else
{
    <h1>No Medicines</h1>
}
